# LLM API Keys (configure the ones you want to use)
GEMINI_API_KEY=your_gemini_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here

# Evaluation Model Configuration
# Options: gemini-1.5-flash, gemini-1.5-pro, claude-3-5-sonnet-20241022, gpt-4o, ollama/qwen2.5-coder
EVAL_MODEL=gemini-1.5-flash

# Ollama Configuration (if using local models)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5-coder:7b

# DeepEval Settings
DEEPEVAL_THRESHOLD=0.7
DEEPEVAL_VERBOSE=true

# Agentcore Configuration
AGENTCORE_API_KEY=your_agentcore_key_here
AGENTCORE_PROJECT_ID=sql-agent-eval

# Strands Configuration
STRANDS_LOG_LEVEL=INFO
